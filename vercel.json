{
  "rewrites": [
    {
      "destination": "/api/auth/$1",
      "source": "/api/auth/(.*)"
    },
    {
      "destination": "/$1",
      "source": "/(.*)"
    },
    {
      "destination": "/api",
      "source": "/api/(.*)"
    },
    {
      "destination": "/api/graphql.js",
      "source": "/api/graphql"
    }
  ]
}